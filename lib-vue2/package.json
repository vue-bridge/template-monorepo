{
  "name": "example-vue-library-vue2",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs.cjs",
  "module": "dist/index.es.js",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "typings": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "script": "./dist/index.iife.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.cjs",
      "default": "./dist/index.es.js"
    },
    "./style.css": "./dist/style.css",
    "./package.json": "./package.json",
    "./src/": "./src/"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && pnpm copy-src",
    "copy-src": "cpy ../lib-vue3/src/* ./dist/src",
    "test": "vitest --reporter=verbose --run",
    "test-watch": "vitest --reporter=verbose"
  },
  "dependencies": {
    "@vue-bridge/runtime": "^0.0.8"
  },
  "devDependencies": {
    "@vue-bridge/testing": "^0.1.1",
    "@vue-bridge/vite-plugin": "^0.0.7",
    "@vue/composition-api": "^1.4.3",
    "@vue/runtime-dom": "latest",
    "@vue/test-utils": "^1.3.0",
    "cpy-cli": "^3.1.1",
    "typescript": "^4.4.4",
    "vite-plugin-vue2": "^1.9.2",
    "vitest": "^0.0.129",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.30.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}