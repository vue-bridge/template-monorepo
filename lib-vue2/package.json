{
  "name": "example-vue-library-vue2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "build:w": "vite build --watch",
    "copy-src": "cpy ../lib-vue3/src/* ./dist/src",
    "test": "vitest --reporter=verbose --run",
    "test-watch": "vitest --reporter=verbose"
  },
  "type": "module",
  "main": "dist/index.cjs.cjs",
  "module": "dist/index.es.js",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "typings": "./typings/main.d.ts",
  "exports": {
    ".": {
      "types": "./typings/main.d.ts",
      "script": "./dist/index.iife.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.cjs",
      "default": "./dist/index.es.js"
    },
    "./style.css": "./dist/style.css",
    "./package.json": "./package.json",
    "./src/": "./src/"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md"
  ],
  "dependencies": {
    "@vue-bridge/runtime": "^0.0.12"
  },
  "devDependencies": {
    "@vue-bridge/testing": "^0.1.2",
    "@vue-bridge/vite-plugin": "^0.1.0",
    "@vue/composition-api": "^1.4.9",
    "@vue/runtime-dom": "^3.2.31",
    "@vue/test-utils": "^1.3.0",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}