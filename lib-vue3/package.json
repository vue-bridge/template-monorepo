{
  "name": "example-vue-library-vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "test": "vitest --reporter=verbose --run",
    "test-watch": "vitest --reporter=verbose",
    "preview": "vite preview"
  },
  "main": "dist/index.vue3.cjs.cjs",
  "module": "dist/index.vue3.es.js",
  "typings": "./typings/main.d.ts",
  "exports": {
    ".": {
      "types": "./index.vue3.d.ts",
      "script": "./dist/index.vue3.iife.js",
      "import": "./dist/index.vue3.es.js",
      "require": "./dist/index.vue3.cjs.cjs",
      "default": "./dist/index.vue3.es.js"
    },
    "./vue3": {
      "types": "./index.vue3.d.ts",
      "script": "./dist/index.vue3.iife.js",
      "import": "./dist/index.vue3.es.js",
      "require": "./dist/index.vue3.cjs.cjs",
      "default": "./dist/index.vue3.es.js"
    },
    "./vue2": {
      "types": "./index.vue2.d.ts",
      "script": "./dist/index.vue2.iife.js",
      "import": "./dist/index.vue2.es.js",
      "require": "./dist/index.vue2.cjs.cjs",
      "default": "./dist/index.vue2.es.js"
    },
    "./style.css": "./dist/style.css",
    "./src/": "./src/",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "typings",
    "index.vue2.d.ts",
    "README.md"
  ],
  "dependencies": {
    "@vue-bridge/runtime": "^0.0.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue-bridge/testing": "^0.1.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "jsdom": "^19.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.0.120",
    "vue": "^3.2.25",
    "vue-tsc": "^0.30.0"
  }
}