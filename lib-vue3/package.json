{
  "name": "example-vue-library-vue3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && pnpm build-types",
    "build-types": "vue-tsc --emitDeclarationOnly",
    "test": "vitest --reporter=verbose --run",
    "test-watch": "vitest --reporter=verbose",
    "preview": "vite preview"
  },
  "type": "module",
  "main": "dist/index.cjs.cjs",
  "module": "dist/index.es.js",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "typings": "./typings/main.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "script": "./dist/index.iife.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.cjs",
      "default": "./dist/index.es.js"
    },
    "./vue3": {
      "types": "./index.d.ts",
      "script": "./dist/index.iife.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.cjs",
      "default": "./dist/index.es.js"
    },
    "./vue2": {
      "types": "./index.d.ts",
      "script": "./dist-vue2/index.iife.js",
      "import": "./dist-vue2/index.es.js",
      "require": "./dist-vue2/index.cjs.cjs",
      "default": "./dist-vue2/index.es.js"
    },
    "./style.css": "./dist/style.css",
    "./src/": "./src/",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "typings",
    "index.vue2.d.ts",
    "README.md"
  ],
  "dependencies": {
    "@vue-bridge/runtime": "^0.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue-bridge/testing": "^0.1.1",
    "@vue-bridge/vite-plugin": "^0.0.6",
    "@vue/test-utils": "^2.0.0-rc.18",
    "typescript": "^4.4.4",
    "vue": "^3.2.25",
    "vue-tsc": "^0.30.0"
  }
}